			СтрокаОтражениеВУчете.СубконтоКт1 = Перечисления.ВидыНачисленийОплатыТрудаДляНУ.пп1ст255;
			СтрокаОтражениеВУчете.СубконтоКт2 = Неопределено;
			СтрокаОтражениеВУчете.СубконтоКт3 = Неопределено;
		Иначе
			СтрокаОтражениеВУчете.СубконтоКт1 = ФизЛицо;
			СтрокаОтражениеВУчете.СубконтоКт2 = Перечисления.ВидыНачисленийОплатыТрудаДляНУ.пп1ст255;
			СтрокаОтражениеВУчете.СубконтоКт3 = Неопределено;
		КонецЕсли;	
	ИначеЕсли ВидОперации = "Договор авторского заказа"
